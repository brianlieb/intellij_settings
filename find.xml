    <mask>*.as</mask>
    <mask>!*.log</mask>
    <mask>*.properties</mask>
    <mask>*.groovy</mask>
    <mask>*.mxml</mask>
    <mask>*.css</mask>
    <mask>*.js</mask>
    <mask>*.java</mask>
    <mask>*.jsp</mask>
    <mask>*.*</mask>
