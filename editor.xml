<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="200" />
    <option name="SMART_BACKSPACE" value="2" />
    <option name="REFORMAT_BLOCK_ON_RBRACE" value="false" />
    <EXCLUDED_PACKAGE NAME="java.util.function.Supplier" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseAccessors" value="true" />
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseI18nMessages" value="false" />
    <option name="collapseLambdas" value="false" />
    <option name="collapseOneLineMethods" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
  <component name="XmlFoldingSettings">
    <option name="COLLAPSE_XML_TAGS" value="true" />
  </component>
</application>