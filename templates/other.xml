<templateSet group="other">
  <template name="doc" value="/**&#10; * $DESCRIPTION$&#10; * &lt;br&gt;&lt;br&gt;&#10; * &lt;b&gt;Inputs:&lt;/b&gt;&lt;br&gt;&#10; * &lt;ul&gt;&#10; *      &lt;li&gt;$INPUT$&lt;/li&gt;&#10; * &lt;/ul&gt;&lt;br&gt;&#10; * &lt;b&gt;Returns:&lt;/b&gt;&lt;br&gt;&#10; * &lt;ul&gt;&lt;li&gt;$OUTPUT$&lt;/li&gt;&lt;/ul&gt;&#10; */" description="Generates a very basic templates to use for Function Javadoc" toReformat="false" toShortenFQNames="true">
    <variable name="DESCRIPTION" expression="" defaultValue="&quot;Functions description&quot;" alwaysStopAt="true" />
    <variable name="INPUT" expression="" defaultValue="&quot;Describe the parameters&quot;" alwaysStopAt="true" />
    <variable name="OUTPUT" expression="" defaultValue="&quot;Enter the returned value&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_COMMENT" value="false" />
    </context>
  </template>
